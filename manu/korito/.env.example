# Auth
AUTH_PROVIDER=supabase
AUTH_JWT_SECRET=your_supabase_jwt_secret
AUTH_JWT_ALGORITHM=HS256
AUTH_JWT_AUDIENCE=authenticated
AUTH_DEFAULT_ROLE=authenticated

# Supabase
SUPABASE_URL=your_supabase_url
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
SUPABASE_SERVICE_KEY=your_supabase_service_key
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_KEY=your_supabase_key
SUPABASE_EMBEDDINGS_TABLE=document_embeddings
SUPABASE_EMBEDDING_COLUMN=embedding
SUPABASE_CONTENT_COLUMN=content
SUPABASE_METADATA_COLUMN=metadata
SUPABASE_CREATED_AT_COLUMN=created_at
SUPABASE_EMBEDDED_FLAG_COLUMN=embedded
EMBEDDED_FLAG_ENABLED=1
SUPABASE_SUMMARIES_TABLE=summaries
SUPABASE_SUMMARY_COLUMN=summary

# Web Search
GOOGLE_SEARCH_API_KEY=your_google_search_api_key
GOOGLE_CSE_CX=your_google_custom_search_id
WEB_SEARCH_PROVIDER=google

# AI Providers
ANTHROPIC_API_KEY=your_anthropic_key
OLLAMA_API_KEY=your_ollama_key
ADMIN_API_KEY=your_admin_key

# OpenAI
OPENAI_API_KEY=your_openai_key
OPENAI_CHAT_MODEL=gpt-4o-mini
EMBEDDING_MODEL=text-embedding-3-small
VECTOR_DIM=1536
EMBEDDING_FALLBACK=0

# RAG tuning
RAG_TOP_K=3
RAG_MIN_SCORE=0.15

# Cultural features
CULTURAL_MODE=true
MAORI_TTS_ENABLED=true

# Ports
BACKEND_PORT=8000
FRONTEND_PORT=5173
